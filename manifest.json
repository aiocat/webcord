{
  "name": "webcord",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://discord.com/channels/*",
        "https://canary.discord.com/channels/*",
        "https://discord.com/app*",
        "https://canary.discord.com/app*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage", "tabs"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Webcord"
  }
}
